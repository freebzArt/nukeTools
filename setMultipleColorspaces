set cut_paste_input [stack 0]
version 12.1 v2
push $cut_paste_input
NoOp {
 name SetMultipleColorspace
 selected true
 xpos 1339
 ypos -2204
 addUserKnob {20 User}
 addUserKnob {3 colorspaceInt t "the number in the list that the colorspace is, start at 0, 1, 2, etc."}
 colorspaceInt 1
 addUserKnob {22 set T "x = nuke.knob('colorspaceInt')\n\nfor node in nuke.selectedNodes():\n    node.knob('colorspace').setValue(x)" +STARTLINE}
 addUserKnob {26 "" +STARTLINE}
 addUserKnob {26 info l "" +STARTLINE T "Enter number of the colorspace you want (starting with 0, 1, 2, etc.) \n\nSelect read nodes\n\nHit \"Set\""}
}
